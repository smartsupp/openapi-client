  "description": "OpenAPI client for {{name}}",
  {{#if repository}}"repository": "{{repository}}",{{/if}}
  {{#if homepage}}"homepage": "{{homepage}}",{{/if}}
  {{#if license}}"license": "{{license}}",{{/if}}
  {{#if author}}"author": "{{author}}",{{/if}}
  {{#if publishConfig}}"publishConfig": {{#json publishConfig}}{{/json}},{{/if}}
